<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="408.0" prefWidth="683.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.registerController">
   <ImageView fitHeight="431.0" fitWidth="683.0" layoutY="-2.0" pickOnBounds="true">
      <Image url="@p.jpg" />
   </ImageView>
   <AnchorPane layoutX="11.0" layoutY="14.0" prefHeight="33.0" prefWidth="361.0" style="-fx-border-color: #227653;">
      <Label layoutX="14.0" layoutY="1.0" prefHeight="30.0" prefWidth="584.0" text="Click On Which Of The Courses You Wish To Register For And Click Again To Delete The Course" textFill="#136013">
         <font>
            <Font name="System Italic" size="11.0" />
         </font>
      </Label>
   </AnchorPane>
   <Button layoutY="394.0" mnemonicParsing="false" onAction="#quit" style="-fx-background-color: transparent;" text="Quit" textFill="#7b7878">
      <font>
         <Font name="System Italic" size="14.0" />
      </font>
   </Button>
   <Button layoutX="120.0" layoutY="396.0" mnemonicParsing="false" onAction="#editInfo" style="-fx-background-color: transpose;" text="Edit Info" textFill="#7b7878">
      <font>
         <Font name="System Italic" size="13.0" />
      </font>
   </Button>
   <Button layoutX="59.0" layoutY="397.0" mnemonicParsing="false" onAction="#logOut" style="-fx-background-color: transparent;" text="LogOut" textFill="#7b6e6e">
      <font>
         <Font name="System Italic" size="12.0" />
      </font>
   </Button>
   <TableView fx:id="courses" layoutX="240.0" layoutY="63.0" onMouseClicked="#courseSelection" prefHeight="331.0" prefWidth="372.0" style="-fx-background-color: white;" />
   <Button layoutX="11.0" layoutY="63.0" mnemonicParsing="false" onAction="#back" prefHeight="30.0" prefWidth="65.0" style="-fx-background-color: #68914b;" text="Back">
      <font>
         <Font name="System Bold Italic" size="14.0" />
      </font>
   </Button>
   <AnchorPane layoutX="83.0" layoutY="63.0" prefHeight="30.0" prefWidth="73.0" style="-fx-border-color: #227653;">
      <children>
         <Label fx:id="unitsTaken" alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="73.0" textFill="#1a9d9d">
            <font>
               <Font size="10.0" />
            </font></Label>
      </children></AnchorPane>
   <Button layoutX="165.0" layoutY="63.0" mnemonicParsing="false" onAction="#done" prefHeight="30.0" prefWidth="65.0" style="-fx-background-color: #68914b;" text="Done">
      <font>
         <Font name="System Bold Italic" size="14.0" />
      </font>
   </Button>
   <Label fx:id="msg" alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="103.0" prefHeight="16.0" prefWidth="195.0" textFill="#08744e">
      <font>
         <Font name="System Italic" size="12.0" />
      </font>
   </Label>
</AnchorPane>
