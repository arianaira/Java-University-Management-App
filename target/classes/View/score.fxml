<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="408.0" prefWidth="683.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.scoreController">
   <ImageView fitHeight="425.0" fitWidth="683.0" layoutY="-2.0" pickOnBounds="true">
      <Image url="@p.jpg"/>
   </ImageView>
   <Button layoutY="394.0" mnemonicParsing="false" onAction="#quit" style="-fx-background-color: transparent;"
           text="Quit" textFill="#7b7878">
      <font>
         <Font name="System Italic" size="14.0"/>
      </font>
   </Button>
   <Button layoutX="112.0" layoutY="395.0" mnemonicParsing="false" onAction="#editInfo"
           style="-fx-background-color: transpose;" text="Edit Info" textFill="#7b7878">
      <font>
         <Font name="System Italic" size="13.0"/>
      </font>
   </Button>
   <Button layoutX="52.0" layoutY="395.0" mnemonicParsing="false" onAction="#LogOut"
           style="-fx-background-color: transparent;" text="LogOut" textFill="#7b6e6e">
      <font>
         <Font name="System Italic" size="13.0"/>
      </font>
   </Button>
   <AnchorPane layoutX="260.0" layoutY="91.0" prefHeight="158.0" prefWidth="201.0" style="-fx-border-color: #227653;">
      <Button layoutX="109.0" layoutY="102.0" mnemonicParsing="false" onAction="#done" prefHeight="30.0"
              prefWidth="65.0" style="-fx-background-color: #68914b;" text="Done">
         <font>
            <Font name="System Bold Italic" size="14.0"/>
         </font>
      </Button>
      <Button layoutX="23.0" layoutY="102.0" mnemonicParsing="false" onAction="#back" prefHeight="30.0"
              prefWidth="65.0" style="-fx-background-color: #68914b;" text="Back">
         <font>
            <Font name="System Bold Italic" size="14.0"/>
         </font>
      </Button>
      <TextField fx:id="score" layoutX="23.0" layoutY="55.0" prefHeight="29.0" prefWidth="151.0" promptText="score"/>
      <Label layoutX="25.0" layoutY="14.0" prefHeight="30.0" prefWidth="174.0" text="Enter Student Score"
             textFill="#3b5320">
         <font>
            <Font name="System Italic" size="16.0"/>
         </font>
      </Label>
   </AnchorPane>
   <Label fx:id="msg" alignment="CENTER" contentDisplay="CENTER" layoutX="260.0" layoutY="260.0" prefHeight="30.0"
          prefWidth="201.0" textFill="#791515">
      <font>
         <Font name="System Italic" size="12.0"/>
      </font>
   </Label>
</AnchorPane>
