<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="408.0" prefWidth="683.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.editCentralManagementController">
   <ImageView fitHeight="438.0" fitWidth="683.0" layoutY="-14.0" pickOnBounds="true">
      <Image url="@9.jpg"/>
   </ImageView>
   <AnchorPane layoutX="225.0" layoutY="65.0" prefHeight="264.0" prefWidth="234.0" style="-fx-border-color: #73cdcd;">
      <TextField fx:id="username" layoutX="16.0" layoutY="36.0" prefHeight="30.0" prefWidth="198.0"
                 promptText="new username"/>
      <TextField fx:id="password" layoutX="16.0" layoutY="102.0" prefHeight="30.0" prefWidth="198.0"
                 promptText="new password"/>
      <Button layoutX="79.0" layoutY="174.0" mnemonicParsing="false" onAction="#submit" prefHeight="46.0"
              prefWidth="77.0" style="-fx-background-color: #73cdcd;" text="Submit">
         <font>
            <Font name="System Bold Italic" size="13.0"/>
         </font>
      </Button>
   </AnchorPane>
   <Button layoutY="394.0" mnemonicParsing="false" onAction="#quit" style="-fx-background-color: transparent;"
           text="Quit" textFill="#7b7878">
      <font>
         <Font name="System Italic" size="14.0"/>
      </font>
   </Button>
   <Button layoutX="47.0" layoutY="397.0" mnemonicParsing="false" onAction="#logOut"
           style="-fx-background-color: transparent;" text="LogOut" textFill="#656464">
      <font>
         <Font name="System Italic" size="12.0"/>
      </font>
   </Button>
</AnchorPane>
