<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="408.0" prefWidth="683.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.coursesController">
   <ImageView fitHeight="425.0" fitWidth="683.0" layoutY="-2.0" pickOnBounds="true">
      <Image url="@p.jpg"/>
   </ImageView>
   <AnchorPane layoutX="254.0" layoutY="61.0" prefHeight="31.0" prefWidth="348.0" style="-fx-border-color: #227653;">
      <Label layoutX="5.0" prefHeight="29.0" prefWidth="339.0"
             text="Select Each Course In Order To See List Of Registered Students" textFill="#20570a">
         <font>
            <Font name="System Italic" size="12.0"/>
         </font>
      </Label>
   </AnchorPane>
   <Button layoutY="394.0" mnemonicParsing="false" onAction="#quit" style="-fx-background-color: transparent;"
           text="Quit" textFill="#7b7878">
      <font>
         <Font name="System Italic" size="14.0"/>
      </font>
   </Button>
   <Button layoutX="112.0" layoutY="395.0" mnemonicParsing="false" onAction="#editInfo"
           style="-fx-background-color: transpose;" text="Edit Info" textFill="#7b7878">
      <font>
         <Font name="System Italic" size="13.0"/>
      </font>
   </Button>
   <Button layoutX="52.0" layoutY="395.0" mnemonicParsing="false" onAction="#LogOut"
           style="-fx-background-color: transparent;" text="LogOut" textFill="#7b6e6e">
      <font>
         <Font name="System Italic" size="13.0"/>
      </font>
   </Button>
   <TableView fx:id="courses" layoutX="254.0" layoutY="102.0" onMouseClicked="#courseSelection" prefHeight="277.0"
              prefWidth="348.0"/>
   <Button layoutX="15.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" prefHeight="30.0" prefWidth="65.0"
           style="-fx-background-color: #68914b;" text="Back">
      <font>
         <Font name="System Bold Italic" size="14.0"/>
      </font>
   </Button>
</AnchorPane>
